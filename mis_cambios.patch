+        <input type="email" name="correo" required class="w-full p-2 border rounded" />
+      </div>
+
+      <button type="submit" class="w-full bg-purple-600 text-white py-2 rounded hover:bg-purple-700">Guardar Docente</button>
+
+      <div class="text-center mt-4">
+        <a href="/dashboard" class="text-pink-600 hover:underline">Volver al Panel de Control</a>
+      </div>
+    </form>
+  </div>
+
+</body>
+</html>
